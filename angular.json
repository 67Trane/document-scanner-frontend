{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "document-scanner-frontend": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/ngx-extended-pdf-viewer/assets/",
                "output": "/assets/"
              }
            ],
            "styles": ["src/styles.css"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2MB",
                  "maximumError": "2MB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "demo": {
              "optimization": true,
              "extractLicenses": false,
              "sourceMap": true,
              "outputPath": "dist/document-scanner-frontend-demo",
              "fileReplacements": [
                {
                  "replace": "src/app/runtime-config.ts",
                  "with": "src/app/runtime-config.demo.ts"
                }
              ]
            },
            "nas": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "outputPath": "dist/document-scanner-frontend-nas",
              "fileReplacements": [
                {
                  "replace": "src/app/runtime-config.ts",
                  "with": "src/app/runtime-config.nas.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "document-scanner-frontend:build:production"
            },
            "development": {
              "buildTarget": "document-scanner-frontend:build:development"
            },
            "demo": {
              "buildTarget": "document-scanner-frontend:build:demo"
            },
            "nas": {
              "buildTarget": "document-scanner-frontend:build:nas"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      }
    }
  }
}
